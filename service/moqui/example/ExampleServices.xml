<?xml version="1.0" encoding="UTF-8"?>
<services xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://moqui.org/xsd/service-definition-2.1.xsd" authenticate="anonymous-all">
    <service verb="create" noun="Example" displayName="Create an Example" type="entity-auto" authenticate="anonymous-all" allow-remote="true">
        <in-parameters>
            <auto-parameters include="non-pk" />
            <parameter name="exampleId" required="true"></parameter>
            <parameter name="exampleName" required="true"></parameter>
            <parameter name="exampleDate" required="true"></parameter>
            <parameter name="examplePrice" required="true">
                <number-decimal/>
            </parameter>
            <parameter name="exampleQuantity" required="true">
                <number-decimal/>
            </parameter>
        </in-parameters>
        <out-parameters>
            <auto-parameters include="pk"/>
        </out-parameters>
    </service>
</services>